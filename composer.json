{
    "name":        "yarkan/webino-image-thumb",
    "description": "Image Thumbnailer for Laminas",
    "homepage":    "https://github.com/webino/WebinoImageThumb",
    "type":        "library",
    "license":     "BSD-3-Clause",
    "keywords":    ["webino", "image", "thumb", "laminas", "framework"],
    "authors": [
        {
            "name":     "Webino, s. r. o.",
            "email":    "info@webino.sk",
            "homepage": "http://webino.sk",
            "role":     "Developer"
        },
        {
            "name":     "Peter Bacinsky",
            "email":    "peter@bacinsky.sk",
            "homepage": "http://bacinsky.sk",
            "role":     "Developer"
        }
    ],
    "minimum-stability": "beta",
    "prefer-stable": true,
    "require": {
        "php": ">=8.1",
        "ext-gd": "*",
        "phpthumb/phpthumb": "dev-master",
        "laminas/laminas-di": "3.*"
    },
    "require-dev": {
        "laminas/laminas-loader": "2.*",
        "laminas/laminas-http": "2.*",
        "laminas/laminas-log": "2.*",
        "laminas/laminas-mvc": "3.*",
        "laminas/laminas-view": "2.*"
    },
    "autoload": {
        "psr-4": {"WebinoImageThumb\\": "src/"}
    },
    "extra": {
        "platform": {
            "php": "8.1.12"
        },
        "webino-devkit": {
            "ZendSkeletonApplication": "release-2.2.8"
        }
    }
}
